<div class="row">
  <div class= "col-md-4 col-md-offset-4">
    <h2 class="text-center"> Entreprise Edition</h2>

    <%= simple_form_for(@entreprise) do |f| %>



      <div class="form-group">
        <%= f.input :name_entreprise,  autofocus: true, placeholder: "name_entreprise", class: "form-control name" %>
      </div>

      <div class="form-group">

        <%= f.input :street_entreprise,  autofocus: true, placeholder: "street_entreprise", class: "form-control name" %>
      </div>

      <div class="form-group">

        <%= f.input :town_entreprise,  autofocus: true, placeholder: "town_entreprise", class: "form-control name" %>
      </div>

      <div class="form-group">

        <%= f.input :zipcode_entreprise,  autofocus: true, placeholder: "zipcode_entreprise", class: "form-control name" %>
      </div>

      <div class="form-group">

        <%= f.input :country_entreprise, as: :text, autofocus: true, placeholder: "country_entreprise", class: "form-control name" %>
      </div>

      <div class="form-group">

        <%= f.input :subdmain,  autofocus: true, placeholder: "subdmain", class: "form-control name" %>
      </div>

      <div><%= f.submit "Update entreprise",class: "btn btn-normal"%></div>
    <% end %>
  </div>
</div>